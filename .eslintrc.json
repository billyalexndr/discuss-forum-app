{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "eol-last": ["error", "always"],
    "indent": ["off"],
    "linebreak-style": ["off", "unix"],
    "import/prefer-default-export": ["off"],
    "no-alert": ["off"]
  }
}
